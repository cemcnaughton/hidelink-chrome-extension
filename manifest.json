{
  "manifest_version": 2,

  "name": "Link Removal",
  "description": "This extension will remove links you do not want",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ],

  
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "matches": [
      "http://*/*",
      "https://*/*"
    ],
    "js": ["jquery.1.11.3.min.js", "onload.js"]
  }]
}